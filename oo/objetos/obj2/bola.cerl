-class(principal).
-export([constructor/0, get_cor/1, get_circun/1,  get_material/1]).

class attributes.

cor
circunferencia
material

class methods.

constructor(Cor, Circun, Material) ->
	cor = Cor,
	circunferencia = Circunferencia,
	material = Material.

get_cor(Key) ->
	cor.

get_circun(Key) ->
	circunferencia.

get_material(Key) ->
	material.
