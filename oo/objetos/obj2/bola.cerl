-class(principal).
-constructor([constructor/0]).
-export([get_cor/1, get_circun/1,  get_material/1]).

class attributes.

cor;
circunferencia;
material;

class methods.

constructor(string Cor, float Circun, string Material) ->
	cor				= Cor,
	circunferencia	= Circunferencia,
	material		= Material.

get_cor() ->
	cor.

get_circun() ->
	circunferencia.

get_material() ->
	material.
