-class(horista).
-extends(empregado).
-export([getSalario/0, calcular/0, main/0]).
-static([main/0]).

class_methods.

getSalario() ->
    -2.

calcular() ->
    io:format("Salario da superclasse: ~p ", [getSalario()]),
    io:format("Salario da subclasse: ~p", [getSalario()]).

main() ->
    H = horista::new(),
    H::calcular().
