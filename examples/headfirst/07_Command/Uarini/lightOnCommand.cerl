-class(lightOnCommand).
-implements(command).
-export([new/1, execute/0, undo/0]).
-constructor([new/1]).

attributes.

Light;
Level.

methods.

new(Light) ->
	self::Light = Light.
	
execute() ->
	Temp = self::Light,
	self::Level = Temp::get_level(),
	Temp::on().

undo() ->
	Temp = self::Light,
	Temp::dim(self::Level).
