-class(runway).
-implements(command).
-export([new/1, land/0]).
-constructor([new/1]).

attributes.

AtcMediator.

methods.

new(AtcMediator) ->
	self::AtcMediator = AtcMediator,
	AtcMediator::set_landing_status(false).
	
land() ->
	AtcMediator = self::AtcMediator,
	AtcMediator::set_landing_status(true),
	io:format("Landing permission granted...~n").
	