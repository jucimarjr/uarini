-class(decoyDuck).
-extends(duck).
-export([new/0, display/0]).
-constructor([new/0]).

methods.

new() ->
	self::QuackBehaviour = muteQuack::new_(),
	self::FlyBehaviour = flyNoWay::new_().

display() ->
	io:format("I'm only a wood carved Decoy Duck...~n").
