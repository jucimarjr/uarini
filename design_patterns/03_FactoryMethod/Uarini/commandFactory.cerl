-class(commandFactory).
-export([create/1]).

class_methods.

create(Nome) ->
	Command = null,
	
	if Nome == "CommandEmail"
		true ->
			Command = commandEmail::constructor();
		false ->
			if Nome == "CommandPDF"
				true ->
					Command = commandPDF::constructor();
				false ->
					nop
		end
	end,
	Command.
