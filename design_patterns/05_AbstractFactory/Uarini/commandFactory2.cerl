-class(commandFactory2).
-export([create/1]).

class_methods.

create(Nome) ->
	Command = null,
	
	if Nome == "CommandEmail"
		true ->
			io:format("Do Aplicativo 2"),
			Command = commandEmail::constructor();
		false ->
			if Nome == "CommandPDF"
				true ->
					io:format("Do Aplicativo 2"),
					Command = commandPDF::constructor();
				false ->
					nop
		end
	end,
	Command.
