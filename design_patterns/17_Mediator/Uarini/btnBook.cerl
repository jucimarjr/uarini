-class(btnBook).
-implements(command).
-constructor([constructor/1]).
-export([execute/0, setEnabled/1]).

class_attributes.

iMediator Med.

class_methods.

constructor(iMediator M) ->
	self::Med = M,
	self::Med::registerBook(self).

execute() ->
	self::Med::book().
	
setEnabled(B) ->
	if(B =:= true)
		true ->
			io:format("BtnBook Enabled");
		false ->
			io:format("BtnBook Disabled")
	end.
